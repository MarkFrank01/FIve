<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <include
        android:id="@+id/ly_personal_info_bar"
        layout="@layout/activity_main_login_navigation_bar" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
               android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/DIMEN_15PX"
                android:layout_marginTop="@dimen/DIMEN_15PX"
                android:layout_height="@dimen/DIMEN_120PX"
                android:orientation="horizontal">
                <TextView
                    style="@style/textview_16_333_center"
                    android:layout_height="match_parent"
                    android:text="企业名称:"
                    />

                <EditText
                    style="@style/textview_16_333_center"
                    android:layout_height="match_parent"
                    android:layout_gravity="left"
                    android:gravity="left|center_vertical"
                    android:layout_marginLeft="@dimen/DIMEN_25PX"
                    android:textSize="@dimen/text_size_60"
                    android:hint="请输入您的企业名称"
                    android:textColorHint="@color/color3"
                    android:background="@null"
                    android:text="qiyemingcheng"/>



            </LinearLayout>

















        </LinearLayout>













    </ScrollView>

</LinearLayout>
