<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/DIMEN_36PX"/>
            <stroke android:width="@dimen/DIMEN_1PX" android:color="@color/colorBlack"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/DIMEN_36PX"/>
            <stroke android:width="@dimen/DIMEN_1PX" android:color="@color/list_item_background_pressed"/>
        </shape>
    </item>

</selector>